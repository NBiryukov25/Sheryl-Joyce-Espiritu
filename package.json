{
  "name": "sheryl-joyce-espiritu",
  "version": "1.0.0",
  "description": "Sheryl Joyce Espiritu's personal website",
  "scripts": {
    "start": "serve",
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass src/styles:dist/css",
    "build:js": "babel src/scripts -d dist/js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write "src/**/*.{js,css,html}""
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "serve": "^14.2.0"
  }
}
